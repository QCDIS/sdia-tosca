{
  "INSTALL_BANK_CHAINCODE": "y",
  "LOG_LEVEL": "INFO",
  "admin_password": "admin1pw",
  "admin_user": "admin1",
  "bank_app": {
    "git_repository": "https://github.com/bityoga/articonf-bank-app.git",
    "image": "bank-app",
    "name": "bank-service",
    "port": 3000,
    "replicas": -1,
    "switch": "on",
    "tag": "latest"
  },
  "caservices": [
    "{{tlsca}}",
    "{{orgca}}"
  ],
  "cli": {
    "image": "hyperledger/fabric-tools",
    "switch": "on",
    "tag": "2.2"
  },
  "couchdb": {
    "image": "couchdb",
    "name": "{{couchdb_user}}",
    "password": "{{couchdb_password}}",
    "path": "/opt/couchdb/data",
    "replicas": -1,
    "switch": "on",
    "tag": "2.3"
  },
  "couchdb_password": "couchdbpw",
  "couchdb_user": "couchdb",
  "explorerservices": [
    "{{hlf_explorer_db}}",
    "{{hlf_explorer}}"
  ],
  "gluster_cluster_volume": "gfs0",
  "glusterd_version": "7",
  "hlf_explorer": {
    "admin_password": "{{hlf_explorer_admin_password}}",
    "admin_user": "{{hlf_explorer_admin_user}}",
    "image": "hyperledger/explorer",
    "name": "hlf_explorer",
    "port": 8090,
    "replicas": -1,
    "switch": "on",
    "tag": "1.1.2",
    "volume": "walletstore"
  },
  "hlf_explorer_admin_password": "adminpw",
  "hlf_explorer_admin_user": "admin",
  "hlf_explorer_db": {
    "db_name": "fabricexplorer",
    "db_password": "{{hlf_explorer_db_password}}",
    "db_user_name": "{{hlf_explorer_db_user}}",
    "image": "hyperledger/explorer-db",
    "name": "hlf_explorer_db",
    "port": 5432,
    "replicas": -1,
    "switch": "on",
    "tag": "1.1.2",
    "volume": "pgdata"
  },
  "hlf_explorer_db_password": "password",
  "hlf_explorer_db_user": "hppoc",
  "orderer": {
    "anchorpeer": "{{peer1.name}}",
    "anchorport": "{{peer1.port}}",
    "caname": "{{orgca.name}}",
    "image": "hyperledger/fabric-orderer",
    "name": "{{orderer_user}}",
    "password": "{{orderer_password}}",
    "path": "/root/{{orderer_user}}",
    "port": 8053,
    "replicas": 1,
    "switch": "on",
    "tag": "2.2",
    "type": "orderer"
  },
  "orderer_password": "ordererpw",
  "orderer_user": "orderer",
  "org": {
    "name": "hlf",
    "unit": "qcdis"
  },
  "orgca": {
    "db": "{{sqlite}}",
    "image": "hyperledger/fabric-ca",
    "name": "{{orgca_user}}",
    "password": "{{orgca_password}}",
    "path": "/root/{{orgca_user}}",
    "port": 8052,
    "replicas": -1,
    "switch": "on",
    "tag": "1.4",
    "type": "org"
  },
  "orgca_password": "orgcapw",
  "orgca_user": "orgca",
  "peer1": {
    "bootstrap": "",
    "caname": "{{orgca.name}}",
    "dbtype": "goleveldb",
    "image": "hyperledger/fabric-peer",
    "leader": "{peer1_user}}",
    "name": "{{peer1_user}}",
    "password": "{{peer1_password}}",
    "path": "/root/{{peer1_user}}",
    "port": 8054,
    "replicas": 1,
    "switch": "on",
    "tag": "2.2",
    "type": "peer"
  },
  "peer1_password": "peer1pw",
  "peer1_user": "peer1",
  "peer2": {
    "bootstrap": "{{peer1.name}}:7051",
    "caname": "{{orgca.name}}",
    "dbtype": "CouchDB",
    "image": "hyperledger/fabric-peer",
    "leader": "{{peer1_user}}",
    "name": "{{peer2_user}}",
    "password": "{{peer2_password}}",
    "path": "/root/{{peer2_user}}",
    "port": 8055,
    "replicas": 1,
    "switch": "on",
    "tag": "2.2",
    "type": "peer"
  },
  "peer2_password": "peer2pw",
  "peer2_user": "peer2",
  "peer3": {
    "bootstrap": "{{peer2.name}}:7052",
    "caname": "{{orgca.name}}",
    "dbtype": "CouchDB",
    "image": "hyperledger/fabric-peer",
    "leader": "{{peer1_user}}",
    "name": "{{peer3_user}}",
    "password": "{{peer3_password}}",
    "path": "/root/{{peer3_user}}",
    "port": 8056,
    "replicas": 1,
    "switch": "on",
    "tag": "2.2",
    "type": "peer"
  },
  "peer3_password": "peer3pw",
  "peer3_user": "peer3",
  "peerservices": [
    "{{peer1}}",
    "{{peer2}}",
    "{{peer3}}"
  ],
  "portainer": {
    "image": "portainer/portainer",
    "name": "portainer",
    "port": 9000,
    "replicas": 1,
    "switch": "on",
    "tag": "latest"
  },
  "portainer_agent": {
    "image": "portainer/agent",
    "name": "portainer_agent",
    "port": 9001,
    "switch": "on",
    "tag": "latest"
  },
  "rest_api": {
    "enable_https": "y",
    "fabric_ca_admin_password": "admin1pw",
    "fabric_ca_admin_user_name": "admin1",
    "git_repository": "https://github.com/bityoga/fabric_as_code_restapi.git",
    "image": "rest-api",
    "name": "rest-api-service",
    "port": 3001,
    "replicas": 1,
    "rest_api_admin_password": "rest_api_admin_password",
    "rest_api_admin_user_name": "rest_api_admin_user",
    "rest_api_admin_user_role": "client",
    "switch": "on",
    "tag": "latest"
  },
  "services": [
    "{{caservices}}",
    "{{orderer}}",
    "{{peerservices}}",
    "{{explorerservices}}",
    "{{vizservices}}"
  ],
  "sqlite": {
    "source": "fabric-ca-server.db",
    "type": "sqlite3"
  },
  "swarm_network": "hlfnet",
  "swarm_visualizer": {
    "image": "dockersamples/visualizer",
    "name": "swarm_visualizer",
    "port": 9090,
    "replicas": 1,
    "switch": "on",
    "tag": "latest"
  },
  "tlsca": {
    "db": "{{sqlite}}",
    "image": "hyperledger/fabric-ca",
    "name": "{{tlsca_user}}",
    "password": "{{tlsca_password}}",
    "path": "/root/{{tlsca_user}}",
    "port": 8081,
    "replicas": 1,
    "switch": "on",
    "tag": "1.4",
    "type": "tls"
  },
  "tlsca_password": "tlscapw",
  "tlsca_user": "tlsca",
  "vizservices": [
    "{{swarm_visualizer}}",
    "{{portainer}}",
    "{{portainer_agent}}"
  ]
}
